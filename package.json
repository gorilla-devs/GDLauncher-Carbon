{
  "name": "corbium",
  "productName": "GDLauncher",
  "private": true,
  "version": "2.0.0",
  "description": "GDLauncher Carbon.",
  "author": "killpowa <davide@gdlauncher.com>",
  "license": "ARR",
  "scripts": {
    "prebuild": "npm i -g pnpm@7.15.0 && PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=true pnpm i && pnpm build-web",
    "dev-app": "turbo run dev --filter=!@gd/web",
    "test": "turbo run test",
    "build-linux-x64": "turbo run build --filter=!@gd/web --filter=!@gd/core --filter=!@gd/app_helper -- --x64 --linux",
    "build-win-x64": "turbo run build --filter=!@gd/web --filter=!@gd/core --filter=!@gd/app_helper -- --x64 --win",
    "build-mac-x64": "turbo run build --filter=!@gd/web --filter=!@gd/core --filter=!@gd/app_helper -- --x64 --mac",
    "build-mac-arm64": "turbo run build --filter=!@gd/web --filter=!@gd/core --filter=!@gd/app_helper -- --arm64 --mac",
    "core-dev": "turbo run dev --filter @gd/core",
    "core-build": "turbo run build --filter @gd/core --filter @gd/app_helper",
    "lint": "turbo run lint --filter @gd/desktop --filter @gd/ui",
    "check-bundle": "bundlewatch --config bundlewatch.config.js",
    "dev-web": "turbo run dev --filter=@gd/web",
    "build-web": "turbo run build --filter @gd/web",
    "lint-web": "turbo run lint --filter @gd/web"
  },
  "devDependencies": {
    "bundlewatch": "^0.3.3",
    "turbo": "^1.6.3"
  },
  "packageManager": "pnpm@7.15.0"
}
