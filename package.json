{
  "name": "corbium",
  "productName": "GDLauncher",
  "private": true,
  "description": "GDLauncher Carbon.",
  "author": "killpowa <davide@gdlauncher.com>",
  "license": "ARR",
  "scripts": {
    "prebuild": "npm i -g pnpm@7.15.0 && PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=true pnpm i && pnpm build-web",
    "dev-app": "turbo run dev --filter=!@gd/web",
    "test": "turbo run test",
    "build-linux-x64": "turbo run build --filter=!@gd/web --filter=!@gd/native_interface -- --x64 --linux",
    "build-win-x64": "turbo run build --filter=!@gd/web --filter=!@gd/native_interface -- --x64 --win",
    "build-mac-x64": "turbo run build --filter=!@gd/web --filter=!@gd/native_interface -- --x64 --mac",
    "build-mac-arm64": "turbo run build --filter=!@gd/web --filter=!@gd/native_interface -- --arm64 --mac",
    "core-dev": "turbo run dev --filter @gd/native_interface",
    "core-build": "turbo run build --filter @gd/native_interface",
    "lint": "turbo run lint --filter @gd/desktop --filter @gd/ui",
    "check-bundle": "bundlewatch --config bundlewatch.config.js",
    "dev-web": "turbo run dev --filter=@gd/web",
    "build-web": "turbo run build --filter @gd/web",
    "lint-web": "turbo run lint --filter @gd/web",
    "prisma-studio": "npx prisma studio --schema ./crates/carbon_app/prisma/schema.prisma",
    "prisma-generate": "cross-env-shell CARGO_TARGET_DIR=./rust_target cargo run -p prisma-cli -- generate --schema=./crates/carbon_app/prisma/schema.prisma",
    "prisma-migrate": "cross-env-shell CARGO_TARGET_DIR=./rust_target cargo run -p prisma-cli -- migrate dev --schema=./crates/carbon_app/prisma/schema.prisma",
    "prisma-reset": "cross-env-shell CARGO_TARGET_DIR=./rust_target cargo run -p prisma-cli -- migrate reset --force --schema=./crates/carbon_app/prisma/schema.prisma",
    "prisma": "cross-env-shell CARGO_TARGET_DIR=./rust_target cargo run -p prisma-cli --"
  },
  "devDependencies": {
    "bundlewatch": "^0.3.3",
    "turbo": "^1.7.0",
    "cross-env": "^7.0.3"
  },
  "packageManager": "pnpm@7.15.0"
}
