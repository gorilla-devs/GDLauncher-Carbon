{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@gd/desktop#build": {
      "dependsOn": ["@gd/core#build", "@gd/ui#build"],
      "outputs": ["dist/**"]
    },
    "@gd/desktop#dev": {
      "dependsOn": ["@gd/core#dev"],
      "outputs": ["dist/**"]
    },
    "@gd/website#dev": {
      "outputs": ["dist/**"]
    },
    "dev": {
      "outputs": ["dist/**"]
    }
  }
}
