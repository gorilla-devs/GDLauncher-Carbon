{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "core.node"],
      "cache": false
    },
    "@gd/desktop#build": {
      "dependsOn": ["^@gd/ui#build", "^@gd/config#build"],
      "cache": false
    },
    "@gd/desktop#dev": {
      "dependsOn": ["^@gd/ui#build", "^@gd/config#build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "@gd/ui#dev": {
      "dependsOn": ["^@gd/config#dev"],
      "cache": false
    },
    "@gd/ui#build": {
      "dependsOn": ["^@gd/config#build"],
      "cache": false
    },
    "@gd/native_interface#dev": {
      "dependsOn": ["@gd/native_interface#codegen"],
      "outputs": ["core.node", "bindings.d.ts"],
      "cache": false
    },
    "@gd/native_interface#build": {
      "dependsOn": ["@gd/native_interface#codegen"],
      "outputs": ["core.node", "bindings.d.ts"],
      "cache": false
    },
    "dev": {
      "outputs": ["dist/**"],
      "cache": false
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "check-bundle": {
      "cache": false
    },
    "codegen": {
      "cache": false,
      "outputs": ["bindings.d.ts"]
    }
  },
  "globalDependencies": [".env"]
}
