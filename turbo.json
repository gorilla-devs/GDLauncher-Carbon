{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "@gd/desktop#build": {
      "dependsOn": ["^@gd/ui#build", "^@gd/config#build"],
      "cache": false
    },
    "@gd/desktop#dev": {
      "dependsOn": ["^@gd/ui#build", "^@gd/config#build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "@gd/ui#dev": {
      "dependsOn": ["^@gd/config#dev"],
      "cache": false
    },
    "@gd/ui#build": {
      "dependsOn": ["^@gd/config#build"],
      "cache": false
    },
    "dev": {
      "outputs": ["dist/**"],
      "cache": false
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "release": {
      "cache": false
    }
  },
  "globalDependencies": [".env"]
}
